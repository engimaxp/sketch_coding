{
  "name": "sketch_coding",
  "version": "0.0.1",
  "description": "coding sketch app (in-dev)",
  "private": true,
  "main": "./main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/engimaxp/sketch_coding.git"
  },
  "author": {
    "name": "name",
    "email": ""
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "redux"
  ],
  "bugs": {
    "url": "https://github.com/engimaxp/sketch_coding/issues"
  },
  "homepage": "https://github.com/engimaxp/sketch_coding",
  "license": "MIT",
  "scripts": {
    "start-renderer": "node scripts/start.js",
    "start-main": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --colors && electron ./main",
    "start": "concurrently --kill-others \"npm run start-renderer\" \"npm run start-main\"",
    "build-renderer": "node scripts/build.js",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --progress --profile --colors",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "local": "electron ./main.js",
    "release-win": "npm run build && electron-builder --win --x64",
    "release-mac": "npm run build && electron-builder --mac --x64",
    "release-all": "npm run build && electron-builder --win --x64 && electron-builder --mac --x64",
    "postinstall": "electron-builder install-app-deps",
    "test": "node scripts/test.js --env=jsdom"
  },
  "build": {
    "productName": "Sketch-Coding",
    "artifactName": "${productName}_setup.${ext}",
    "appId": "com.enpfighter.app",
    "files": [
      "build/",
      "main.js",
      "package.json"
    ],
    "mac": {
      "identity": null
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.1.3",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "codemirror": "^5.48.4",
    "connected-react-router": "^4.4.1",
    "css-loader": "0.28.7",
    "dexie": "^2.0.4",
    "dotenv": "8.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "1.3.0",
    "fs-extra": "3.0.1",
    "highlight.js": "^9.15.10",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "is-boolean-object": "^1.0.0",
    "isomorphic-git": "^0.55.2",
    "jest": "22.4.2",
    "lodash": "^4.17.13",
    "lodash-es": "^4.17.14",
    "lodash.template": "^4.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "nock": "^10.0.6",
    "object-assign": "4.1.1",
    "pako": "^1.0.10",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.8.0",
    "react-codemirror2": "^6.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "5.0.2",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.3",
    "react-select-material-ui": "^4.0.0",
    "react-split-pane": "^0.1.87",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "remarkable": "^1.7.1",
    "resolve": "1.10.1",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "superagent": "^5.0.5",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/codemirror": "^0.0.76",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "24.0.12",
    "@types/lodash": "^4.14.134",
    "@types/mkdirp": "^0.5.2",
    "@types/nock": "^10.0.2",
    "@types/node": "11.13.9",
    "@types/pako": "^1.0.1",
    "@types/react": "16.8.17",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "7.0.8",
    "@types/react-router": "5.0.0",
    "@types/react-router-dom": "4.3.3",
    "@types/react-select": "^2.0.17",
    "@types/redux-logger": "3.0.6",
    "@types/redux-mock-store": "^1.0.1",
    "@types/remarkable": "^1.7.0",
    "@types/sinon": "^7.0.11",
    "@types/superagent": "^4.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^7.2.4",
    "electron-builder": "^20.28.3",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "redux-logger": "^3.0.6",
    "typescript": "^3.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/app/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
